{
    "name": "typewriter",
    "displayName": "Typewriter",
    "description": "Human-like code code typer",
    "version": "0.0.1",
    "publisher": "dansilver",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.playback",
        "onCommand:extension.setTypewriter"
    ],
    "main": "./out/src/extension",

    "properties": {
        "typewriter.TypingMinSpeed": {
            "type": "number",
            "default": 30,
            "description": "Lower limit on how fast to type - randomized between this and max"
        },
        "typewriter.TypingMaxSpeed": {
            "type": "number",
            "default": 120,
            "description": "Upper limit on how fast to type - randomized between min and this"
        }
    },
    "contributes": {
        "commands": [{
            "command": "extension.playback",
            "title": "Typewriter: Play"
        }, {
            "command": "extension.setTypewriter",
            "title": "Typewriter: Set"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}